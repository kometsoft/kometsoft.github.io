---
import Layout from '../layouts/Layout.astro'
import { Image } from 'astro:assets'
import { getCollection } from 'astro:content'

const posts = (await getCollection('blog')).sort((a, b) => b.data.publishDate.valueOf() - a.data.publishDate.valueOf()).slice(0, 3)
---

<Layout title="Welcome to Kometsoft (M) Sdn. Bhd.">
  <div class="bg-white pb-6 sm:pb-8 lg:pb-12 flex items-center min-h-[calc(100vh-64px)] sm:min-h-[calc(100vh-76px)]">
